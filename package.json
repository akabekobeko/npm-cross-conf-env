{
  "name": "cross-conf-env",
  "description": "To cross-platform the config and root variable reference of package.json in npm-scripts.",
  "version": "1.1.2",
  "author": "akabeko",
  "license": "MIT",
  "homepage": "https://github.com/akabekobeko/npm-cross-conf-env#readme",
  "engines": {
    "node": ">= 4"
  },
  "main": "src/lib/index.js",
  "bin": "src/bin/index.js",
  "files": [
    "src/bin",
    "src/lib",
    "index.js"
  ],
  "keywords": [
    "npm-scripts",
    "npm_package_config",
    "package.json",
    "variable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akabekobeko/npm-cross-conf-env.git"
  },
  "bugs": {
    "url": "https://github.com/akabekobeko/npm-cross-conf-env/issues"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./esdoc",
    "test": {
      "type": "mocha",
      "source": "./test"
    },
    "plugins": [
      {
        "name": "esdoc-node"
      }
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "scripts": {
    "test": "mocha --timeout 50000 --compilers js:babel-register test/**/*.test.js",
    "esdoc": "esdoc",
    "eslint": "eslint ./src"
  },
  "dependencies": {
    "cross-spawn": "^5.1.0"
  },
  "devDependencies": {
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.23.0",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
  }
}
