{
  "name": "cross-conf-env",
  "description": "To cross-platform the config and root variable reference of package.json in npm-scripts.",
  "version": "1.0.6",
  "author": "akabeko",
  "license": "MIT",
  "homepage": "https://github.com/akabekobeko/npm-cross-conf-env#readme",
  "main": "index.js",
  "bin": "bin/main.js",
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "keywords": [
    "npm-scripts",
    "npm_package_config",
    "package.json",
    "variable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akabekobeko/npm-cross-conf-env.git"
  },
  "bugs": {
    "url": "https://github.com/akabekobeko/npm-cross-conf-env/issues"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "scripts": {
    "test": "mocha --timeout 50000 --compilers js:babel-register test/**/*.test.js",
    "start": "npm run watch",
    "esdoc": "esdoc -c esdoc.json",
    "build": "babel src --out-dir ./",
    "watch": "babel src --out-dir ./ --watch",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.14.0",
    "esdoc": "^0.4.8",
    "mocha": "^3.0.2",
    "power-assert": "^1.4.1"
  },
  "dependencies": {
    "cross-spawn": "^4.0.0"
  }
}
